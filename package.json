{
  "name": "@authenio/samlify-xmllint-wasm",
  "version": "1.0.0",
  "description": "Validator module powered by xmllint-wasm",
  "main": "build/index.js",
  "scripts": {
    "build": "yarn run clean && yarn run cp-schemas && tsc",
    "cp-schemas": "mkdir -p build/schemas && cp -R schemas build",
