  "bugs": {
    "url": "https://github.com/authenio/samlify-xmllint-wasm/issues"
  },
  "homepage": "https://github.com/authenio/samlify-xmllint-wasm#readme",
  "devDependencies": {
    "@types/node": "^20.3.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "xmllint-wasm": "^4.0.0"
  }
}
